{
  "name": "closest-to-viewport-center",
  "description": "Given a list of DOM elements, return the one closest to the center of the browser viewport",
  "version": "1.0.0",
  "author": "Eric Lanehart <eric@pushred.co> (http://pushred.co)",
  "license": "ISC",
  "main": "dist/closet-to-viewport-center.js",
  "module": "closet-to-viewport-center.js",
  "scripts": {
    "build": "babel ./closest-to-viewport-center.js -d dist",
    "demo": "serve .",
    "jest": "jest *.test.js --env=node --runInBand",
    "test": "run-p demo jest --race"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "add-module-exports",
      "transform-es2015-modules-umd"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "jest": "^21.1.0",
    "nightmare": "^2.10.0",
    "npm-run-all": "^4.1.1",
    "serve": "^6.1.0"
  }
}
